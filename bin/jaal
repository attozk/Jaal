#!/usr/bin/env php
<?php

memprof_enable();

require __DIR__ . '/../vendor/autoload.php';

$doc = <<<DOC
Jaal.

Usage:
  jaal [run] [--] <filename>
  jaal debug [--] <filename>
  jaal (-h | --help)

Options:
  -h --help     Show this screen.
DOC;

$args = Docopt::handle($doc);

Hathoora\Jaal\Jaal::execute($args);

echo "\n";